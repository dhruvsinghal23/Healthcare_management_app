        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }
   var bgr = new GlideAjax('FetchBillingDetails');
   bgr.addParam('sysparm_billing_id',newValue);
   bgr.addParam('sysparm_name','getBillingInfo');
   bgr.getXMLAnswer(function(response)
   {
	var result = JSON.parse(response);
	var mainResult = 'The amount due is '+ result.amount_due + ' ' + result.success + ' ' + result.status;
	g_form.showFieldMsg('billing_id',mainResult,'info');
	g_form.setValue('appointment_id',result.appointment_id);
	g_form.setValue('patient_id',result.patient_id);
   }
   );

   //Type appropriate comment here, and begin script below
   
}]]></script>
