        <script><![CDATA[var PalindromeChecker = Class.create();
PalindromeChecker.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

    isPalindrome: function() {
        var input = this.getParameter('sysparm_input_name');
        var reversed = input.split('').reverse().join(''); 
        var result = (input == reversed) ? 'true' : 'false';
        return result;
    }

});
