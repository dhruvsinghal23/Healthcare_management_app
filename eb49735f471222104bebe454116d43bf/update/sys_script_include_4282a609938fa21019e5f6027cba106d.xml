        <script><![CDATA[var Questionexample = Class.create();
Questionexample.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

	getPatientDetails:function()
	{
		var patientId = this.getParameter('sysparm_patient_id');
		var gr = new GlideRecord('u_patient_record');
		if(gr.get(patientId))
		{
			var result ={
				name : gr.getValue('u_first_name'),
				age : gr.getValue('u_age1')
			};
			return new global.JSON().encode(result);
		}
		return 'not Found';
	},
    type: 'Questionexample'
});]]></script>
